                    android:text="@string/prompt_maca_pht_stepmother"
                    android:textSize="@dimen/text_size_hint" />

                <CheckBox
                    android:id="@+id/pht_father"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0"
                    android:checked="false"
                    android:text="@string/prompt_maca_pht_father"
                    android:textSize="@dimen/text_size_hint" />

                <CheckBox
                    android:id="@+id/pht_stepfather"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0"
                    android:checked="false"
                    android:text="@string/prompt_maca_pht_stepfather"
                    android:textSize="@dimen/text_size_hint" />

